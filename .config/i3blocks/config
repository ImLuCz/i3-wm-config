[cpu]
command=grep 'cpu ' /proc/stat | awk '{usage=($2+$4)*100/($2+$4+$5)} END {pr>
interval=2

[mem]
command=free -m | awk '/^Mem/ { printf("RAM: %.1f%%\n", $3/$2 * 100.0) }'
interval=5

[volume]
label=VOL:
command=amixer get Master | awk -F'[][]' '/%/ { print $2; exit }'
interval=1
signal=10

[battery]
label=BAT:
command=acpi -b | awk '{print $4}' | sed 's/,//'
interval=30

[time]
command=date '+%a %d %H:%M'
interval=60# Gotta keep it real, chatgpt took over for this one

[cpu]
command=grep 'cpu ' /proc/stat | awk '{usage=($2+$4)*100/($2+$4+$5)} END {printf("CPU: %.1f%%\n", usage)}'
interval=2

[mem]
command=free -m | awk '/^Mem/ { printf("RAM: %.1f%%\n", $3/$2 * 100.0) }'
interval=5

[battery]
command=acpi -b | awk '{print "Battery: " $4}' | sed 's/,//'
interval=30

[time]
command=date '+%a %d %H:%M'
interval=60
